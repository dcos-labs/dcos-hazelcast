{  
      "id":"hazelcast",
      "cpus":0.5,
      "mem":2000,
      "instances":3,
      "container":{  
        "type":"DOCKER",
        "docker":{  
          "image":"unterstein/marathon-hazelcast",
          "forcePullImage":true
        },
        "portMappings":[
            {
               "hostPort":0,
               "containerPort":5701,
               "protocol":"tcp"
            }
         ]
      },
      "healthChecks":[
        {
          "protocol":"HTTP",
          "path":"/hazelcast/health",
          "portIndex":0,
          "timeoutSeconds":10,
          "gracePeriodSeconds":240,
          "intervalSeconds":10,
          "maxConsecutiveFailures":10
        }
      ]
    }