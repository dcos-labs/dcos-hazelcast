{
  "id": "load-generator",
  "cpus": 0.5,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "unterstein/dcos-hazelcast-load-generator",
      "network": "BRIDGE",
      "forcePullImage": true,
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 0,
      "timeoutSeconds": 10,
      "gracePeriodSeconds": 240,
      "intervalSeconds": 10,
      "maxConsecutiveFailures": 10
    }
  ],
  "env": {
    "VIP_APP": "http://sample.marathon.l4lb.thisdcos.directory:8080",
    "AMOUNT": "10000"
  }
}