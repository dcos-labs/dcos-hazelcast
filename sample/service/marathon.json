{  
      "id":"sample",
      "cpus":0.5,
      "mem":512,
      "instances":1,
      "container":{  
        "type":"DOCKER",
        "docker":{  
          "image":"unterstein/dcos-hazelcast-sample",
          "network":"BRIDGE",
          "forcePullImage":true,
          "portMappings":[
            {
               "hostPort":0,
               "containerPort":8080,
               "protocol":"tcp"
            }
         ]
        }
      },
      "healthChecks":[
        {
          "protocol":"HTTP",
          "path":"/",
          "portIndex":0,
          "timeoutSeconds":10,
          "gracePeriodSeconds":240,
          "intervalSeconds":10,
          "maxConsecutiveFailures":10
        }
      ],
      "labels":{
          "HAPROXY_0_VHOST":"junterste-publicsl-am99k1hheb7f-1110651884.us-west-2.elb.amazonaws.com",
          "HAPROXY_GROUP":"external"
      }
    }